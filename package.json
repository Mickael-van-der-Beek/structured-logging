{
  "name": "structured-logging",
  "version": "3.0.0",
  "description": "Bunyan stream used to format logger input based on a JSON schema.",
  "keywords": [
    "bunyan",
    "logger",
    "logging",
    "pruning",
    "schema",
    "serializer"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.1.0"
  },
  "scripts": {
    "updates": "npm-check --skip-unused",
    "audit": "retire --path .",
    "coverage": "nyc npm run unit",
    "lint": "semistandard --verbose | snazzy",
    "mutate": "stryker run stryker.conf.js",
    "test": "npm run updates && npm run audit && npm run lint && npm run coverage && npm run mutate",
    "unit": "mocha --recursive ./test/**/*.spec.js"
  },
  "dependencies": {
    "ajv": "^4.11.5",
    "dot-object": "^1.5.4",
    "response-time": "^2.3.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "elasticsearch": "^12.1.3",
    "mocha": "^3.2.0",
    "npm-check": "^5.4.0",
    "nyc": "^10.1.2",
    "retire": "^1.2.12",
    "semistandard": "^10.0.0",
    "snazzy": "^6.0.0",
    "stryker": "^0.5.9",
    "stryker-api": "^0.4.2",
    "stryker-mocha-runner": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Woorank/structured-logging.git"
  },
  "author": "Mickael van der Beek (https://github.com/Mickael-van-der-Beek)",
  "bugs": {
    "url": "https://github.com/Woorank/structured-logging/issues"
  },
  "homepage": "https://github.com/Woorank/structured-logging"
}
