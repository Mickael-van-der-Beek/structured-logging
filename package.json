{
  "name": "structured-logging",
  "version": "2.0.15",
  "description": "Initialises a list of serializers and middlewares used to format logger input.",
  "keywords": [
    "buyan",
    "logger",
    "logging",
    "serializer"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": ">=4.2.6"
  },
  "scripts": {
    "ci": "npm run lint && npm run coverage",
    "coverage": "nyc npm run test",
    "lint": "semistandard --verbose | snazzy",
    "start": "node ./server.js",
    "test": "mocha --recursive ./test/**/*.spec.js"
  },
  "dependencies": {
    "ajv": "^4.11.3",
    "bunyan": "^1.8.1",
    "dot-object": "^1.5.4",
    "extend": "^3.0.0",
    "ip": "^1.1.3",
    "proxy-addr": "^1.1.3",
    "response-time": "^2.3.1",
    "urijs": "^1.18.8",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "express": "^4.15.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "semistandard": "^9.2.1",
    "snazzy": "^6.0.0",
    "supertest": "^3.0.0",
    "request-promise": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Woorank/structured-logging.git"
  },
  "author": "Mickael van der Beek (https://github.com/Mickael-van-der-Beek)",
  "bugs": {
    "url": "https://github.com/Woorank/structured-logging/issues"
  },
  "homepage": "https://github.com/Woorank/structured-logging",
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
