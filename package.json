{
  "name": "structured-logging",
  "version": "1.0.0",
  "description": "Initialises a list of serializers and middlewares used to format logger input.",
  "keywords": [
    "buyan",
    "logger",
    "logging",
    "serializer"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": "~4.2.6"
  },
  "scripts": {
    "ci": "npm run security && npm run lint && npm run test",
    "lint": "semistandard --verbose | snazzy",
    "security": "snyk test .",
    "start": "node ./server.js",
    "test": "mocha --recursive ./test",
    "cover": "nyc npm run test"
  },
  "dependencies": {
    "ajv": "~4.1.7",
    "bunyan": "~1.8.1",
    "extend": "^3.0.0",
    "ip": "~1.1.3",
    "proxy-addr": "~1.1.2",
    "response-time": "~2.3.1",
    "uuid": "~2.0.2"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "express": "~4.14.0",
    "mocha": "~2.5.3",
    "nyc": "~7.0.0",
    "semistandard": "^8.0.0",
    "sinon": "^1.17.4",
    "snazzy": "^4.0.0",
    "snyk": "~1.17.1",
    "supertest": "~1.2.0",
    "validator": "~5.4.0",
    "request-promise": "~4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Woorank/structured-logging.git"
  },
  "author": "Mickael van der Beek (https://github.com/Mickael-van-der-Beek)",
  "bugs": {
    "url": "https://github.com/Woorank/structured-logging/issues"
  },
  "homepage": "https://github.com/Woorank/structured-logging"
}
